#include <stdio.h>

int main() {
    int n,i,c =0;
    printf("Enter any number n:");
    scanf("%d",&n);
    for(i=1; i<=n;i++)
    {
        if(n % i==0)
        {
            c++;
        }
    }
    if (c==2)
    {
        printf("n is a prime number");
    }
        else
        {
        printf ("n is not a prime not a prime number");
        }
        return 0;
        }

  output:1
Enter any number n:5
n is a prime number

output:2
Enter any number n:15
n is not a prime not a prime number
